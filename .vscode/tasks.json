{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: arm-linux-gnueabihf-g++ build active file",
			"command": "/opt/gnueabi/opt/ext-toolchain/bin/arm-linux-gnueabihf-g++",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}",
				"-std=c++11"
			],
			"options": {
				"cwd": "/opt/gnueabi/opt/ext-toolchain/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /opt/gnueabi/opt/ext-toolchain/bin/arm-linux-gnueabihf-g++"
		}
	]
}